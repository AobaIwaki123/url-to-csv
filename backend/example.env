# Net2Sheet Backend Environment Variables
# Copy this file to .env and fill in your values

# Google Cloud Project Configuration
PROJECT_ID=your-project-id-here
REGION=us-central1

# Cloud Storage
BUCKET=net2sheet-uploads-your-suffix

# Cloud Run Service Configuration
SERVICE_NAME=net2sheet-upload
JOB_NAME=csv-to-sheets-job

# Google Sheets Configuration
SHEET_ID=your-google-sheet-id-here
SHEET_RANGE=Sheet1!A1

# JWT Authentication
JWT_SECRET=change-me-to-a-long-random-string-for-production
JWT_EXPIRES_IN=1h

# CORS Configuration (for Chrome Extension)
ALLOWED_ORIGINS=chrome-extension://*

# Development/Local Testing
PORT=8080

# Example values for reference:
# PROJECT_ID=my-net2sheet-project
# BUCKET=net2sheet-uploads-20250101
# SHEET_ID=1BxiMVs0XRA5nFMdKvBdBZjgmUUqptlbs74OgvE2upms
# JWT_SECRET=your-very-long-random-secret-key-here-at-least-32-characters
